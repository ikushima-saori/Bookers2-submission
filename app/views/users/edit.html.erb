<h1>users/edit　この行は後で消す</h1>
<div class="ユーザーの編集">
  <h1>User info</h1>
  <%= form_with model: @user do |f| %>
  <div>
    <label for="inputName">Name</label>
    <%= f.text_field :name, autofocus: true, id:"inputName"%>
  </div>
  <div>
    <label>Image</label>
    <%= f.file_field :profile_image, accept: "image/*" %>
  </div>
  <div>
    <label for="inputIntroduction">Introduction</label>
    <%= f.text_field :introduction, id:"inputIntroduction"%>
  </div>
  <div>
    <%= f.submit "Update User" %>
  </div>
  <% end %>
</div>